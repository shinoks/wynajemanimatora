{% if app.session is defined %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="alert-success alert with-close alert-{{ type }} alert-dismissable show">
                <div class="containter">
                    {{ message }}
                </div>
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}
